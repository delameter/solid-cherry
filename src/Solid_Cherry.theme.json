      "underlineColor": "cherry_7",
      "inactiveUnderlineColor": "cherry_6",
      "underlineHeight": 2
    },

    "StatusBar": {
      "hoverBackground": "solid_5"
    },

    "VersionControl": {
      "RefLabel.foreground": "label_5",
      "Log.Commit": {
        "currentBranchBackground": "23213b",
        "unmatchedForeground": "label_3"
      },
      "FileHistory.Commit.selectedBranchBackground": "cherry_5"
    },

    "FileColor": {
      "Green": "212621",
      "Yellow": "2C1D1F",
      "Red": "2C1D1F"
