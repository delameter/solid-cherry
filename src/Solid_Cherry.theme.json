      "pressedBackground": "solid_7",
      "pressedBorderColor": "solid_7"
    },

    "Button": {
      "foreground": "label_7",
      "arc": 8,
      "focusedBorderColor": "cherry_6",
      "shadowColor": "transparent",
      "startBackground": "solid_7",
      "endBackground": "solid_7",
      "startBorderColor": "solid_7",
      "endBorderColor": "solid_7",
      "default": {
        "foreground": "label_7",
        "startBackground": "cherry_6",
        "endBackground": "cherry_6",
        "focusedBorderColor": "cherry_6",
        "startBorderColor": "transparent",
        "endBorderColor": "transparent"
      }
    },

    "Table.stripeColor": "solid_4",

    "Tree": {
      "rowHeight": 20
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "solid_4"
      },

      "HeaderTab": {
        "underlinedTabBackground": "solid_5"
      },

      "Button": {
        "hoverBackground": "solid_5",
        "selectedBackground": "solid_5"
      }
    },

    "Popup": {
