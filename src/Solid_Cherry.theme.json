        "activeBackground": "solid_5",
        "inactiveBackground": "solid_5"
      }
    },

    "DefaultTabs": {
      "hoverBackground": "transparent",
      "underlineColor": "cherry_7",
      "underlinedTabBackground": "solid_5",
      "underlineHeight": 2
    },

    "SearchEverywhere": {
      "Tab": {
        "selectedForeground": "label_5",
        "selectedBackground": "solid_5"
      }
    },

    "Plugins": {
      "Tab": {
        "selectedBackground": "solid_5",
        "hoverBackground": "solid_5"
      },
      "lightSelectionBackground": "solid_5"
    },

    "EditorTabs": {
      "selectedBackground": "solid_5",
      "underlinedTabBackground": "solid_5",
